{
  "name": "tsholosetati-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,js}\"",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "chart.js": "^4.4.0"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": "prettier --write"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "typescript": "^5.1.6",
    "eslint": "^8",
    "@typescript-eslint/parser": "^6",
    "@typescript-eslint/eslint-plugin": "^6",
    "eslint-config-prettier": "^9",
    "prettier": "^3",
    "husky": "^8",
    "lint-staged": "^13"
  },
  "engines": {
    "node": ">=18"
  }
}
